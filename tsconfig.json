{
  "compilerOptions": {
    "baseUrl": "presentation",
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ],
    "downlevelIteration": true
  }
}
